<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.netsurf">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_public_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".CheckBalanceActivity" />
        <activity android:name=".ShareDataActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".LoginHistoryActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".BuyDataActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".Lightningbolt" />
        <activity android:name=".Popup"></activity>
    </application>

</manifest>